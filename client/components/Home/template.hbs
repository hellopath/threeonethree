					<div class="title-container title">
						<div class="vertical-center-parent">
							<div class="vertical-center-child">{{{title}}}</div>
						</div>
					</div>
					<div class="slides-container">
						{{#each images}}
							<div class="slide">
								<img src="media/slideshow/{{url}}">
