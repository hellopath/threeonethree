<div id="home-page" class="page-wrapper">
	<div class="all-slides-holder">
		<div class="top-slide slide-block">
			<table id="logo-wrapper">
				<tr>
					<td><img src={{logoUrl}} alt="" /></td>
				</tr>
			</table>
			<div class="arrow-holder">
